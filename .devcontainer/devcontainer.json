{
    "name": "myjulia",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "mhutchie.git-graph",
                "julialang.language-julia",
                "ms-toolsai.jupyter"
            ],
            "settings": {
                "workbench.colorTheme": "Default Light Modern",
                "julia.executablePath": "/usr/local/julia/bin/julia",
                "editor.acceptSuggestionOnEnter": "off"
            }
        }
    }
}
